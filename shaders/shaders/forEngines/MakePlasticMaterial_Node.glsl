void MakePlasticMaterial(
    vec3 Color,
    vec3 specularcolor,
    float specularstrength,
    float shininess,
    // Base Parameters
    out vec3 BaseColor,
    out vec3 SpecularColor,
    out float SpecularStrength,
    out float Shininess
){
    BaseColor = Color;
    SpecularColor = SpecularColor;
    SpecularStrength = SpecularStrength;
    Shininess = shininess;
    
}